{
  "expo": {
    "name": "catia",
    "slug": "catia",
    "owner": "theomoura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "catia",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-router",
      [
        "@stripe/stripe-react-native",
        {}
      ],
      "expo-font",
      "expo-web-browser",
      [
        "@siteed/expo-audio-studio",
        {
          "enableBackgroundAudio": true,
          "enableNotifications": true,
          "enablePhoneStateHandling": true,
          "enableDeviceDetection": false,
          "iosBackgroundModes": {
            "useAudio": true,
            "useProcessing": true
          },
          "iosConfig": {
            "microphoneUsageDescription": "CatIA precisa de acesso ao microfone para gravar sessões de áudio."
          }
        }
      ]
    ],
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anonymous.catia"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1bdbd89b-aae0-4acb-86cb-b48877059d9e"
      }
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.catia",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "CatIA precisa de acesso ao microfone para gravar sessões de áudio."
      }
    }
  }
}